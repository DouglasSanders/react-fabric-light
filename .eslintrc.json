{
  "root": true,
  "plugins": ["react"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.0"
    }
  },
  "rules": {},
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "globals": {
    "session": true,
    "arguments": true,
    "process": true,
    "__dirname": true,
    "global": true
  }
}
